<?xml version="1.0" encoding="utf-8"?>

<!-- 
	for details of supported settings, see 
	http://r-c4d3.googlecode.com/svn/trunk/_documentation/romloader-api/ConfigDataProxy.html 
-->
<configuration>


	<!-- trace logging on when true -->
	<logging enabled="true" />

	<!-- performance statistics on when true and played in debug player -->
	<stats enabled="true" />

	<!-- rom to load -->
	<rom file="./rom.swf" 
	     id="r-c4d3.hungries.desktop" />

	<!-- not defining key mappings, using default values from romloader -->
	<keymappings />
	
	<!-- custom properties go here, e.g.
		<properties>
			<property name="fullScreen">false</property>
		</properties>
	-->
	<properties />


</configuration>
