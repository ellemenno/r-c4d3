<?xml version="1.0" encoding="utf-8"?>

<!--
	Config file for R-C4D3 romloader.
	
	see documentation at:
	http://r-c4d3.googlecode.com/svn/trunk/_documentation/romloader-api/ConfigDataProxy.html
  -->
<configuration>


	<!-- trace logging on when true -->
	<logging enabled="true" />

	<!-- rom to load -->
	<rom file="./rom.swf" id="test_romloader_desktop" />

	<!-- not defining key mappings, using default values from romloader -->
	<keymappings />
	
	<!-- custom properties go here, e.g.
		<properties>
			<property name="fullScreen">false</property>
		</properties>
	-->
	<properties />


</configuration>
